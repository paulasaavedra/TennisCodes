top3_por_año <- resultados[
order(-Ganados),
head(.SD, 2),
by = year
]
# Solo jugadores con al menos 10 victorias en el año
top3_efectividad <- resultados[Ganados >= 10][
order(-`% ganados`),
head(.SD, 3),
by = year
]
View(resultados)
